<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tabbed_graph"
    app:startDestination="@id/tabbed1Fragment">
    <androidx.fragment.app.FragmentContainerView
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:id="@+id/nav_host_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true"
        app:navGraph="@navigation/tabbed_graph" />
    <fragment
        android:id="@+id/tabbed1Fragment"
        android:name="com.example.p3navigation.Tabbed1Fragment"
        android:label="fragment_tabbed1"
        tools:layout="@layout/fragment_tabbed1" />
</navigation>